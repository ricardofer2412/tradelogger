            <label>
               Market Price
               
            
                <p id='marketPrice'> {{quoteData.c}} </p>
            </label>
            <label>
                Shares:
                <input type='number' id:'sharesNumber' name='sharesNumber' />
            </label>
        
            <h4>Estimated Cost = </h4>
      
            <button type='submit'>Buy</button>
