

<div>
    <div class='graph'>
        <div>
            <img src={{companyData.logo}} alt="">
        <h2>{{companyData.name}}</h2>
        <h3>{{quoteData.c}}</h3>
        </div>
        <div class=''>
            This is graph
        </div>
        <div>
            <p>Buying Power</p>
            <p>$100,000</p>
        </div>
    </div>
  
    <div class='stockList-div'>
        <h4>Buy {{companyData.ticker}}</h4>
